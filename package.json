{
  "name": "crayfish",
  "productName": "123",
  "description": "bug-repo",
  "author": "Martin Myhrman",
  "homepage": "www.myhrmans.com",
  "license": "MIT",
  "version": "0.0.1",
  "main": "dist/index.js",
  "scripts": {
    "nodemon": "nodemon",
    "start": "npm run compile && electron .",
    "serve:svelte": "cd svelte && npm run dev",
    "svelte:build": "cd svelte && npm run build",
    "compile": "tsc",
    "out:linux": "npm run svelte:build && npm run compile && electron-builder build --linux --publish never",
    "publish:linux": "npm run svelte:build && npm run compile && electron-builder build --linux --publish always",
    "out:win": "npm run svelte:build && npm run compile && electron-builder build --win --publish never",
    "publish:win": "npm run svelte:build && npm run compile && electron-builder build --win --publish always",
    "postinstall": "electron-builder install-app-deps",
    "dev-start": "run-p nodemon serve:svelte"
  },
  "devDependencies": {
    "concurrently": "^6.4.0",
    "electron": "^16.0.1",
    "electron-builder": "^22.14.5",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.2",
    "typescript": "^4.5.2"
  },
  "dependencies": {
    "electron-reload": "^1.5.0",
    "electron-serve": "^1.1.0",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.3.9",
    "npm-run-all": "^4.1.5"
  },
  "build": {
    "appId": "123",
    "directories": {
      "output": "out"
    },
    "publish": [
      {
        "provider": "github",
        "owner": "myhrmans",
        "repo": "123"
      }
    ],
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "icons/win/icon.ico"
    },
    "linux": {
      "category": "TODO: fill here the category of your app",
      "icon": "icons/png/",
      "target": [
        "AppImage"
      ]
    },
    "nsis": {
      "installerIcon": "icons/win/icon.ico",
      "uninstallerIcon": "icons/win/icon.ico",
      "uninstallDisplayName": "123",
      "license": "license.txt",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    }
  }
}
